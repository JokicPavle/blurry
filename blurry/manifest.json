{
  "name": "Blurry",
  "action": {},
  "manifest_version": 3,
  "version": "0.1",
  "description": "Blur images and videos on the page",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "host_permissions": ["https://*/*","http://*/*"]
}